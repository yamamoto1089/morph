<div class="admin_index">
	<h1 class="page_title">ユーザー管理画面</h1>
    <div class="user_list">
        <% @users.each do |users| %>
            <p>
            <%= users.user_name %>
            <%= users.email %>
            <%= link_to users.user_name, user_path(users)%>
            </p>
        <%end%>
    </div>
</div>